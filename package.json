{
  "name": "solana-froth",
  "displayName": "Solana Froth",
  "description": "Info about Solana blockchain and wallets.",
  "version": "0.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sirius-Hash/solana-froth-vscode.git"
  },
  "publisher": "SiriusHash",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:solana-froth.refresh",
    "onCommand:solana-froth.refresh-view",
    "onView:solana-froth-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sidebar-view",
          "title": "Solana Froth",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "sidebar-view": [
        {
          "type": "webview",
          "id": "solana-froth-sidebar",
          "name": "Solana Froth",
          "icon": "media/icon.svg",
          "contextualTitle": "Solana Froth"
        }
      ]
    },
    "commands": [
      {
        "command": "solana-froth.refresh",
        "category": "Solana Froth",
        "title": "Refresh"
      },
      {
        "command": "solana-froth.refresh-view",
        "category": "Solana Froth",
        "title": "Refresh View"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rollup -c && webpack --mode production --devtool hidden-source-map",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/codicons": "^0.0.32",
    "@vscode/test-electron": "^2.2.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
